{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "my-extensions",
  "title": "My extensions",
  "description": "original extensions",
  "icon": "command-icon.png",
  "author": "ocucraqp",
  "license": "MIT",
  "commands": [
